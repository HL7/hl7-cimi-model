definition
	ProposalAgainst[id1.1.1] matches {
    type matches {
          Concept[id2] matches {code matches {[ac1.1.1]}}
    }
	  validityPeriod matches {
	 	 IntervalValue<DateTime>[id0.0.1]
	  }
	}

terminology
	term_definitions = <
		["en"] = <
			["id1.1.1"] = <
				text = <"Proposal against">
				description = <"Concept represents a recommendation from a clinical decision support system or advice from a consultation to not perform an act.">
			>
			["id0.0.1"] = <
				text = <"ValidityPeriod">
				description = <"The time period during which the act should not be performed.">
			>
      ["ac1.1.1"] = <
        text = <"Proposed against context range">
        description = <"Value set containing a single code indicating that the action was proposed against (410521004 |Not to be done (qualifier value)|.">
      >
		>
	>

